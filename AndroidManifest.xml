<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yahoo.ads.reporting"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.yahoo.ads.reporting.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.yahoo.ads.reporting.VendorListActivity"
            android:label="@string/title_activity_vendors" >
        </activity>
        <activity
            android:name="com.yahoo.ads.reporting.PlacementListActivity"
            android:label="@string/title_activity_placement_list" >
        </activity>
        <activity
            android:name="com.yahoo.ads.reporting.PlacementDetailsActivity"
            android:label="@string/title_activity_placement_details" >
        </activity>
        <activity
            android:name="com.yahoo.ads.reporting.TrendActivity"
            android:label="@string/title_activity_trend" >
        </activity>
        
        <activity android:name="org.achartengine.GraphicalActivity" />
    </application>

</manifest>
